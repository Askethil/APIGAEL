<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Utilisateurs" table="utilisateurs">
    <id name="idutilisateur" type="integer" column="idUtilisateur">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mdp" type="string" column="mdp" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idboutique" type="integer" column="idBoutique" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
